                <TableColumn fx:id="produsColoana" prefWidth="181.0" text="Produs" />
                <TableColumn fx:id="lemnColoana" prefWidth="183.0" text="Lemn" />
                <TableColumn fx:id="pretColoana" prefWidth="219.0" text="Pret" />
